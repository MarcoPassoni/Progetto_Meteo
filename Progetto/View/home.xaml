<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:local="clr-namespace:Progetto.View"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Class="Progetto.View.home">
    <TabBar>
        <Tab Title="Options" Icon="option.png">
            <ShellContent ContentTemplate="{DataTemplate local:Options}"/>
        </Tab>
        <Tab Title="Home" Icon="home.png">
            <ShellContent ContentTemplate="{DataTemplate local:DefaultPage}"/>
        </Tab>
        <Tab Title="Search" Icon="search.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
        </Tab>
    </TabBar>
</Shell>